type Chat = variant {
  Group : principal;
  Channel : record { principal; nat32 };
  Direct : principal;
};
type InitOrUpgradeArgs = record {
  chat : Chat;
  oc_api_key : text;
  bot_canister_id : principal;
  administrator : principal;
};
type Result = variant { Ok; Err : text };
type SyncApiKeyArgs = record { oc_api_key : text };
service : (InitOrUpgradeArgs) -> { sync_api_key : (SyncApiKeyArgs) -> (Result) }
